{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_author}"],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": ["com.lattice.Lattice"]
  },
  "mixins": [
    "lattice.mixins.json",
    "lattice.mixins.compat.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric-api": ">=${fabric_api_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
    "refinedstorage": "*",
    "sophisticatedstorage": "*",
    "sophisticatedbackpacks": "*"
  }
}
